{
  "name": "line-login-go",
  "description": "Line login PKCE sample code",
  "repository": "https://github.com/kkdai/line-login-pkce-go",
  "keywords": ["LINE", "go", "static"],
  "buildpacks": [
    {
      "url": "https://github.com/kr/heroku-buildpack-go.git"
    },
    {
      "url": "heroku/go"
    }
  ],
  "env": {
    "LINECORP_PLATFORM_CHANNEL_CHANNELID": {
      "description": "LINE Login - Channel ID",
      "required": true
    },
    "LINECORP_PLATFORM_CHANNEL_CHANNELSECRET": {
      "description": "LINE Login - Channel secret",
      "required": true
    },
    "LINECORP_PLATFORM_CHATBOT_CHANNELTOKEN": {
      "description": "LINE Chatbot - token",
      "required": true
    },
    "LINECORP_PLATFORM_CHATBOT_CHANNELSECRET": {
      "description": "LINE Chatbot - secret",
      "required": true
    },
    "LINECORP_PLATFORM_SERVERURL": {
      "description": "Server URL",
      "required": true
    }
  }
}
